\" option=\"seasonalDecomp\"/>\n            <Target action=\"disable\" conditionResult=\"false\" option=\"seasonalDecomp\"/>\n            <Target action=\"hide\" conditionResult=\"true\" option=\"seasonalStatsNote\"/>\n            <Target action=\"show\" conditionResult=\"false\" option=\"seasonalStatsNote\"/>\n        </Dependency>\n        <Dependency condition=\"$decompChkbox=='1'\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"selectDecompPlotsCombo\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"selectDecompPlotsCombo\"/>\n        </Dependency>\n        <Dependency condition=\"$selectDecompPlotsCombo=='selectedDecompPlots'\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"decompPlotsGroup\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"decompPlotsGroup\"/>\n        </Dependency>\n        <Dependency condition=\"$componentsPlot=='1'\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"componentSelect\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"componentSelect\"/>\n        </Dependency>\n        <Dependency condition=\"$decompPanel=='1' || $componentSelect.size()&gt;=1 || $seasonAdjSeriesPlot=='1' || $pctChangeSeasonAdjSeriesPlot=='1' || $selectDecompPlotsCombo=='defaultDecompPlots'\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"decompMethod\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"decompMethod\"/>\n        </Dependency>\n        <Dependency condition=\"$spectralPlot=='1'\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"minperiodCombo\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"minperiodCombo\"/>\n            <Target action=\"show\" conditionResult=\"true\" option=\"spectraDetailsGroup\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"spectraDetailsGroup\"/>\n        </Dependency>\n        <Dependency condition=\"$kernelFunc=='none'\">\n            <Target action=\"hide\" conditionResult=\"true\" option=\"scaleKernelCombo\"/>\n            <Target action=\"show\" conditionResult=\"false\" option=\"scaleKernelCombo\"/>\n            <Target action=\"hide\" conditionResult=\"true\" option=\"exponentCombo\"/>\n            <Target action=\"show\" conditionResult=\"false\" option=\"exponentCombo\"/>\n        </Dependency>\n        <Dependency condition=\"$scaleKernelCombo=='scaleKernelCustomChoice'\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"scaleKernel\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"scaleKernel\"/>\n        </Dependency>\n        <Dependency condition=\"$exponentCombo=='exponentCustomChoice'\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"exponent\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"exponent\"/>\n        </Dependency>\n        <Dependency condition=\"$adfTest=='1'\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"arOrder\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"arOrder\"/>\n        </Dependency>\n        <Dependency condition=\"$minperiodCombo=='minperiodCustomChoice'\">\n            <Target action=\"show\" conditionResult=\"true\" option=\"minperiod\"/>\n            <Target action=\"hide\" conditionResult=\"false\" option=\"minperiod\"/>\n        </Dependency>\n        <Dependency condition=\"$timeID.size()&gt;=1 &amp;&amp; $dependVariable.size()&gt;0 &amp;&amp; !($varSettings.get('accumulate').get(&quot;$dependVariable[0]&quot;)=='accumulateNone')\">\n            <Target action=\"enable\" conditionResult=\"true\" option=\"trendStats\"/>\n            <Target action=\"disable\" conditionResult=\"false\" option=\"trendStats\"/>\n            <Target action=\"hide\" conditionResult=\"true\" option=\"trendStatsNote\"/>\n            <Target