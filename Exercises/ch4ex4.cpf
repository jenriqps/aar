ity!=\"\" && $seasonality!=\"1\") seasonality=$seasonality #end #end #end\n#if($plotsNeeded==\"1\") plots=(\n\t#if($seriesPlot==\"1\") series #end #if($seriesHistogram==\"1\") histogram #end #if($seasonalCyclesPlot==\"1\") cycles #end \n\t#if($spectralPlot==\"1\") spectrum #if($minperiodCombo==\"minperiodCustomChoice\") (minperiod=$minperiod) #end #end\n\t#if($autoPanel==\"1\" || $defaultAutocorrPlot==\"1\") corr #end\n\t#if($acfPlot==\"1\" || $normAcfPlot==\"1\") acf #end #if($pacfPlot==\"1\" || $normPacfPlot==\"1\") pacf #end #if($iacfPlot==\"1\" || $normIacfPlot==\"1\") iacf #end #if($wnPlot==\"1\" || $wnLogPlot==\"1\") wn #end\n\t#if($decompPanel==\"1\" || $defaultDecompPlot==\"1\") decomp #end \n\t#if($seasonAdjSeriesPlot==\"1\") sa #end #if($pctChangeSeasonAdjSeriesPlot==\"1\") pcsa #end\t\n\t#if($componentsPlot==\"1\") \n\t\t#if($componentSelect.contains(\"trendComp\")) tc #end #if($componentSelect.contains(\"cycleComp\")) cc #end #if($componentSelect.contains(\"trendCycleComp\")) tcc #end\n\t\t#if($componentSelect.contains(\"seasonalComp\")) sc #end #if($componentSelect.contains(\"irregularComp\")) ic #end #if($componentSelect.contains(\"seasonIregularComp\")) sic #end\n\t\t#if($componentSelect.contains(\"trendCycleSeasonComp\")) tcs #end\n\t#end)\n#end\n#if($crossPlotsSelected==\"1\") crossplots=(#if($crossSeriesPlot==\"1\") series #end #if($crossCorrFuncPlot==\"1\" || $normCrossCorrFuncPlot==\"1\") ccf #end) #end\n#if($statsSelected==\"1\") print=(#if($descriptiveStats==\"1\") descstats #end #if($seasonalStats==\"1\") seasons #end #if($seasonalDecomp==\"1\") decomp #end #if($trendStats==\"1\") trends #end) #end;\n#if($timeID.size()>0)\n\tid #foreach($item in $timeID) $item #end interval=$timeIDProps.intervalName;\n#end\n#foreach($item in $dependList) \n\tvar $CTMUtil.toSASName($item.variable) / \n\t#if($item.accumulate) accumulate=#if($item.accumulate==\"accumulateAvg\") average #elseif($item.accumulate==\"accumulateTotal\") total #else none #end #end\n\t#if($item.transformation) transform=#if($item.transformation==\"logTrans\") log #elseif($item.transformation==\"sqrtTrans\") sqrt \n\t\t#elseif($item.transformation==\"logisticTrans\") logistic #elseif($item.transformation==\"boxcoxTrans\") boxcox(0) #else none #end #end\n\t#if($item.simpleDiff) dif=$item.simpleDiff #end\n\t#if($item.seasonalDiff) sdif=$item.seasonalDiff #end;\n#end\n#if($autocorrNlagsCombo==\"autocorrNlagsCustomChoice\") corr / nlag=$autocorrNlags; #end\n#foreach($item in $independList) \n\tcrossvar $CTMUtil.toSASName($item.variable) / \n\t#if($item.accumulate) accumulate=#if($item.accumulate==\"accumulateAvg\") average #elseif($item.accumulate==\"accumulateTotal\") total #else none #end #end\n\t#if($item.transformation) transform=#if($item.transformation==\"logTrans\") log #elseif($item.transformation==\"sqrtTrans\") sqrt \n\t\t#elseif($item.transformation==\"logisticTrans\") logistic #elseif($item.transformation==\"boxcoxTrans\") boxcox(0) #else none #end #end\n\t#if($item.simpleDiff) dif=$item.simpleDiff #end\n\t#if($item.seasonalDiff) sdif=$item.seasonalDiff #end;\n#end\n#if($crosscorrNlagsCombo==\"crosscorrNlagsCustomChoice\") crosscorr / nlag=$crosscorrNlags; #end\n#if($decompStmtNeeded==\"1\") \n\tdecomp #if($seasonAdjSeriesPlot==\"1\") adjusted #end #if($pctChangeSeasonAdjSeriesPlot==\"1\") pcsa #end \n\t\t#if($componentSelect.size()>=1) \n\t\t\t#if($componentSelect.contains(\"trendComp\")) tc #end \n\t\t\t#if($componentSelect.contains(\"cycleComp\")) cycle #end \n\t\t\t#if($componentSelect.contains(\"trendCycleComp\")) trendcycle #end\n\t\t\t#if($componentSelect.contains(\"seasonalComp\")) seasonal #end \n\t\t\t#if($componentSelect.contains(\"irregularComp\")) irregular #end \n\t\t\t#if($componentSelect.contains(\"seasonIregularComp\")) seasonirregular #end\n\t\t\t#if($componentSelect.contains(\"trendCycleSeasonComp\")) trendcycleseason #end\n\t\t#end\n\t/ #if($decompMethod) mode=$decompMethod #end;\n#end\n## ods exclude to separate autocor